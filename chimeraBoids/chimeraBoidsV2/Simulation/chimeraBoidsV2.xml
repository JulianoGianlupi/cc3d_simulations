<CompuCell3D Revision="20180722" Version="3.7.8">
   
   <Potts>
      
      <!-- Basic properties of CPM (GGH) algorithm -->
      <Dimensions x="256" y="256" z="1"/>
      <Steps>10000</Steps>
      <Temperature>10.0</Temperature>
      <NeighborOrder>2</NeighborOrder>
      <Boundary_x>Periodic</Boundary_x>
      <Boundary_y>Periodic</Boundary_y>
<!--       <RandomSeed>582155538</RandomSeed> -->
   </Potts>
   <Metadata>
   <DebugOutputFrequency>100</DebugOutputFrequency>
</Metadata>

   
   
   <Plugin Name="CellType">
      
      <!-- Listing all cell types in the simulation -->
      <CellType TypeId="0" TypeName="Medium"/>
      <CellType TypeId="1" TypeName="boidsA"/>
   </Plugin>
   
   <Plugin Name="Volume"/>
   
   <Plugin Name="ExternalPotential">
      
      <!-- External force applied to cell. Each cell has different force and force components have to be managed in Python. -->
      <!-- e.g. cell.lambdaVecX=0.5; cell.lambdaVecY=0.1 ; cell.lambdaVecZ=0.3; -->
      <Algorithm>PixelBased</Algorithm>
   </Plugin>
   
   <Plugin Name="CenterOfMass">
      
      <!-- Module tracking center of mass of each cell -->
   </Plugin>
   
   <Plugin Name="NeighborTracker">
      
      <!-- Module tracking neighboring cells of each cell -->
   </Plugin>
   
   <Plugin Name="Contact">
      <!-- Specification of adhesion energies -->
      <Energy Type1="Medium" Type2="Medium">10.0</Energy>
      <Energy Type1="Medium" Type2="boidsA">10.0</Energy>
      <Energy Type1="boidsA" Type2="boidsA">22.0</Energy>
      <NeighborOrder>3</NeighborOrder>
   </Plugin>
   
